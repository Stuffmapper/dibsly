<div class="full-height" >

  <div  ng-include src="'navbar.html'"></div> 


  <div class="col-md-4 col-lg-4
  col-sm-4 col-xs-12 pull-right
  stuffmapper-menu"
  ng-class="menuHeight ">
    <ul class="sm-tabs">
      <li class="col-md-4 col-lg-4 col-xs-4 tab-unclicked" ui-sref-active="tab-clicked" >      
        <a ui-sref='getStuff' >
          <h4>Get Stuff</h4>
        </a>
      </li>

      <li class="col-md-4 col-lg-4 col-xs-4 tab-unclicked" ui-sref-active="tab-clicked" >
        <a ui-sref='giveStuff' >
          <h4>Give Stuff</h4>
        </a>
      </li>
      <li class="col-md-4 col-lg-4 col-xs-4 tab-unclicked" ui-sref-active="tab-clicked" >
        <a ui-sref='myStuff'>
          <h4>My Stuff</h4>
        </a>
      </li>

      <alerts></alerts>
    </ul>

    <div ui-view>
    </div>



    <div ng-show='tabs.details[0]'class='details-container' >
      <div class="col-lg-12 col-md-12 col-xs-12 post-details" >
        <smdetails post='stuff' ></smdetails>
      </div>
    </div>


    <edit ng-show='tabs.editing[0]' post='editItem' ></edit>

	</div>


<!-- google map -->
  <div class="col-md-8 col-lg-8 col-sm-8 col-xs-12 pull-left" ng-class='mapHeight' id='google-container'>
    <span class="glyphicon glyphicon-screenshot center-map"
      ng-click='centerMap()'
      aria-hidden="true">
    </span>
    <div id="google-map">
    </div>
  </div>


</div>
